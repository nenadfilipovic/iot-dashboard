{
  "name": "Authentication-Service",
  "service": "authentication-service",
  "dockerComposeFile": [
    "../../../../docker-compose.yml",
    "../../../../docker-compose.devcontainer.yml"
  ],
  "settings": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },
  "extensions": [
    "esbenp.prettier-vscode",
    "dbaeumer.vscode-eslint",
    "Prisma.prisma"
  ],
  "forwardPorts": [3005],
  "postCreateCommand": "sh .devcontainer/refresh-container.sh",
  "remoteUser": "node",
  "shutdownAction": "none",
  "workspaceFolder": "/workspace/application/services/authentication"
}
