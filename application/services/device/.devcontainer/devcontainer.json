{
  "name": "Device-Service",
  "service": "device-service",
  "dockerComposeFile": [
    "../../../../docker-compose.yml",
    "../../../../docker-compose.devcontainer.yml"
  ],
  "settings": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },
  "extensions": ["esbenp.prettier-vscode", "dbaeumer.vscode-eslint"],
  // "forwardPorts": [],
  "postCreateCommand": "sh .devcontainer/refresh.sh",
  "remoteUser": "node",
  "shutdownAction": "none",
  "workspaceFolder": "/workspace/application/services/device"
}
