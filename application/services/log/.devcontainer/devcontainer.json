{
  "name": "Log-Service",
  "service": "log-service",
  "dockerComposeFile": [
    "../../../../docker-compose.yml",
    "../../../../docker-compose.dev.yml"
  ],
  "settings": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "extensions.ignoreRecommendations": true,
    "editor.formatOnSaveMode": "modifications"
  },
  "extensions": ["esbenp.prettier-vscode", "dbaeumer.vscode-eslint"],
  "forwardPorts": [3003],
  "postCreateCommand": "sh .devcontainer/refresh.sh",
  "remoteUser": "node",
  "shutdownAction": "stopCompose",
  "workspaceFolder": "/workspace/application/services/log"
}
