{
  "name": "Log-Service",
  "service": "log-service",
  "dockerComposeFile": [
    "../../../../docker-compose.yml",
    "../../../../docker-compose.devcontainer.yml"
  ],
  "settings": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },
  "extensions": ["esbenp.prettier-vscode", "dbaeumer.vscode-eslint"],
  "forwardPorts": [3015],
  "postCreateCommand": "sh .devcontainer/refresh-container.sh",
  "remoteUser": "node",
  "shutdownAction": "none",
  "workspaceFolder": "/workspace/application/services/log"
}
