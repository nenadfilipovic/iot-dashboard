generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int         @default(autoincrement()) @id
  user_id       String      @unique
  name          String
  email         String      @unique
  password      String
  image         String?
  location_lat  Float?
  location_long Float?
  status        User_status @default(active)
  modified_at   DateTime    @default(now())
  created_at    DateTime    @default(now())
}

enum User_status {
  active
  disabled
}
